{
    "name": "lekoala/ss-gridfield-utils",
    "description": "A collection of GridField components that you can use with any GridField, including Editable Rows and amy more",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "utilities",
        "gridfield"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mellisa Hankins",
            "email": "mell@milkywaymultimedia.com.au"
        },
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "silverstripe/recipe-plugin": "^1 || ^2",
        "silverstripe/vendor-plugin": "^1 || ^2",
        "silverstripe/framework": "^4.4 || ^5",
        "symbiote/silverstripe-gridfieldextensions": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "replace": {
        "milkyway-multimedia/ss-gridfield-utils": "*"
    }
}
